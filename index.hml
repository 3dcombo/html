        body {
            font-family: 'Inter', sans-serif;
            background-color: #f7f7fa; /* A light background color */
        }
        .ai-tool-card {
            background-color: #ffffff;
            border-radius: 12px;
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.05);
            transition: transform 0.2s, box-shadow 0.2s;
        }
        .ai-tool-card:hover {
            transform: translateY(-3px);
            box-shadow: 0 6px 15px rgba(0, 0, 0, 0.08);
        }
        .tag {
            display: inline-flex;
            align-items: center;
            padding: 4px 8px;
            border-radius: 6px;
            font-size: 0.75rem;
            font-weight: 500;
            margin-right: 6px;
            margin-bottom: 6px;
        }
        .tag.free-trial { background-color: #e0f2fe; color: #0284c7; } /* light blue */
        .tag.freemium { background-color: #fef3c7; color: #d97706; } /* light yellow */
        .tag.paid { background-color: #fee2e2; color: #dc2626; } /* light red */
        .tag.category { background-color: #e2e8f0; color: #475569; } /* light gray */
        .header-gradient {
            background: linear-gradient(to right, #6a82fb, #fc5c7d);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }
        .section-gradient {
            background: linear-gradient(to right, #6a82fb, #fc5c7d);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }
        .footer-link {
            color: #4b5563;
            text-decoration: none;
            transition: color 0.2s;
        }
        .footer-link:hover {
            color: #6366f1; /* purple-500 */
        }
        .social-icon {
            color: #6b7280;
            transition: color 0.2s;
        }
        .social-icon:hover {
            color: #6366f1;
        }
    </style>
